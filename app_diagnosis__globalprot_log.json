{
    "querystring": {
        "p_l": 50,
        "p_c": true,
        "o_1": "-timestamp",
        "o_2": "-id",
        "s_cliente__exact": "globalprot",
        "s_tipo__startswith": "gpsync"
    },
    "schema": {},
    "title": "Log Drupal",
    "layout": {
        "botonProcesos": {
            "componente": "YBButton",
            "label": "Ir a procesos",
            "buttonType": "raised",
            "action": {
                "key": "gotoproccess"
            }
        },
        "botonErroneos": {
            "componente": "YBButton",
            "label": "Ir a erróneos",
            "buttonType": "raised",
            "action": {
                "key": "gotoerror"
            }
        },
        "logGrid": {
            "componente": "YBGrid",
            "class": "claseGrid",
            "prefix": "yb_log",
            "colorRowField": "rowColor",
            "filter": {
                "tipo": {
                    "filterType": "multiseleccion",
                    "title": "Tipo sincro",
                    "values": {
                        "gpsyncproducts": "Sincro Productos",
                        "gpsyncusers": "Sincro Clientes",
                        "gpsyncorders": "Sincro Pedidos"
                    }
                },
                "texto": {
                    "filterType": "multiseleccion",
                    "title": "Excluir del log",
                    "criterio": "nin",
                    "values": {
                        "Éxito. No hay datos que sincronizar": "No hay datos",
                        "Éxito. No es momento de sincronizar": "No es momento",
                        "Info. Proceso detenido": "Detenido",
                        "Info. Proceso arrancado": "Arrancado"
                    }
                }
            },
            "paginacion": true,
            "columns": [
                {"tipo": "field", "key": "timestamp", "flex": 1, "listpos": "body"},
                {"tipo": "field", "key": "texto", "flex": 5, "listpos": "subtitle", "avatar": true, "title": true},
                {"tipo": "field", "key": "tipo", "flex": 1, "listpos": "title"}
            ]
        }
    },
    "acciones": {
        "gotoproccess": {
            "action": "url",
            "url": "diagnosis/yb_procesos/custom/globalprot_drupal"
        },
        "gotoerror": {
            "action": "url",
            "url": "diagnosis/yb_procesos_erroneos/custom/globalprot_drupal_error"
        }
    }
}
