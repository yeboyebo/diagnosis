{
    "querystring": {
        "p_l": 50,
        "p_c": true,
        "o_1": "-timestamp",
        "o_2": "-id",
        "s_cliente__exact": "elganso",
        "s_tipo__startswith": "egsyncvt"
    },
    "schema": {},
    "title": "Log Ventas",
    "layout": {
        "groupBoxAcciones": {
            "componente": "YBGroupBox",
            "className": "actionGB",
            "layout": {
                "botonProcesos": {
                    "componente": "YBButton",
                    "label": "Ir a procesos",
                    "buttonType": "raised",
                    "className": "floatLeft",
                    "action": {
                        "key": "gotoproccess"
                    }
                },
                "botonErroneos": {
                    "componente": "YBButton",
                    "label": "Ir a err√≥neos",
                    "buttonType": "raised",
                    "className": "floatLeft",
                    "action": {
                        "key": "gotoerror"
                    }
                }
            }
        },
        "logGrid": {
            "componente": "YBGrid",
            "class": "claseGrid",
            "prefix": "yb_log",
            "colorRowField": "rowColor",
            "filter": "buscador",
            "paginacion": true,
            "columns": [
                {"tipo": "field", "key": "timestamp", "flex": 1, "listpos": "body"},
                {"tipo": "field", "key": "texto", "flex": 5, "listpos": "subtitle", "avatar": true},
                {"tipo": "field", "key": "tipo", "flex": 1, "listpos": "title"}
            ]
        }
    },
    "acciones": {
        "gotoproccess": {
            "action": "url",
            "url": "diagnosis/yb_procesos/custom/elganso_ventas"
        },
        "gotoerror": {
            "action": "url",
            "url": "diagnosis/yb_procesos_erroneos/custom/elganso_ventas_error"
        }
    }
}
