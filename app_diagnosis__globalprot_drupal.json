{
    "querystring": {
        "p_l": 50,
        "p_c": true,
        "o_1": "proceso",
        "s_cliente__exact": "globalprot",
        "s_proceso__startswith": "gpsync"
    },
    "schema": {},
    "title": "Drupal",
    "layout": {
        "groupBoxAcciones": {
            "componente": "YBGroupBox",
            "className": "actionGB",
            "layout": {
                "botonLog": {
                    "componente": "YBButton",
                    "label": "Ir a log",
                    "buttonType": "raised",
                    "className": "floatLeft",
                    "action": {
                        "key": "gotolog"
                    }
                }
            }
        },
        "procesosGrid": {
            "componente": "YBList",
            "class": "claseGrid",
            "prefix": "yb_procesos",
            "filter": "buscador",
            "paginacion": true,
            "columns": [
                {"tipo": "field", "listpos": "subtitle", "key": "proceso", "flex": 1},
                {"tipo": "field", "listpos": "title", "key": "descripcion", "flex": 5},
                {"tipo": "field", "listpos": "secondaryitem", "key": "activo", "flex": 1, "avatar": true},
                {"tipo": "foreignfield", "key": "ultsincro", "flex": 1, "listpos": "body"},
                {
                    "tipo": "act",
                    "key": "start",
                    "label": "Iniciar",
                    "success": [{"slot": "refrescar", "custom": "globalprot_procesos"}]
                },
                {
                    "tipo": "act",
                    "key": "stop",
                    "label": "Parar",
                    "success": [{"slot": "refrescar", "custom": "globalprot_procesos"}]
                }
            ],
            "accionesGrid": [
                {
                    "tipo": "button",
                    "key": "stopall",
                    "label": "Parar todos",
                    "success": [{"slot": "refrescar"}]
                },
                {
                    "tipo": "button",
                    "key": "startall",
                    "label": "Iniciar todos",
                    "success": [{"slot": "refrescar"}]
                }
            ]
        }
    },
    "acciones": {
        "start": {
            "action": "legacy",
            "prefix": "yb_procesos",
            "serverAction": "start",
            "icon": "play_circle_filled"
        },
        "stop": {
            "action": "legacy",
            "prefix": "yb_procesos",
            "serverAction": "stop",
            "icon": "pause_circle_outline"
        },
        "startall": {
            "action": "gridAction",
            "prefix": "yb_procesos",
            "serverAction": "startall"
        },
        "stopall": {
            "action": "gridAction",
            "prefix": "yb_procesos",
            "serverAction": "stopall"
        },
        "gotolog": {
            "action": "url",
            "url": "diagnosis/yb_log/custom/globalprot_log"
        }
    }
}
